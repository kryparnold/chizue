// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Channel {
    id     String   @id
    player String
    letter String
    limit  Int
    type   GameType
    words  String[]
}

enum GameType {
    WORDGAME
    COUNTING
}
